    function Student ({phone,name,id,persentage}){
        this.phone= phone;
        this.id = id;
        this.name = name;
        this.persentage = persentage ;
    }

    Student.prototype.study = function(){
        console.log(this.name + ' is studing');
    }

    var students = [];
